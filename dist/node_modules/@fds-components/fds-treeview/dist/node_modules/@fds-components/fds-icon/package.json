{
  "name": "@fds-components/fds-icon",
  "version": "1.1.15",
  "private": false,
  "scripts": {
    "build": "npm run clear && npm run prepare && rollup -c",
    "devdoc": "npm run clear && npm run prepare && export MAKE_DOC=true; rollup -c -w",
    "dev": "npm run clear && npm run prepare && rollup -c -w",
    "clear": "rm -rf dist",
    "prepare": "mkdir -p dist/svgs && npm run copy:all-icons && mkdir -p dist/node_modules &&  npm run copy:node_modules &&  npm run copy:json_doc",
    "copy:node_modules": "cp -rp node_modules/@fds-components dist/node_modules",
    "copy:json_doc": "find . -maxdepth 1 -name \"*.json\" ! -name 'package*' ! -name 'COMPONENT_API.json' -type f  -exec cp -- {} dist \\;",
    "copy:all-icons": "npm run copy:icons-from-src && npm run copy:bootstrap-icons && npm run copy:fab5 && npm run copy:far5 && npm run copy:fas5 && npm run copy:flags",
    "copy:icons-from-src": "cp -rp src/svgs dist",
    "copy:bootstrap-icons": "cp -rp node_modules/bootstrap-icons/icons dist/svgs/bi",
    "copy:fab5": "cp -rp node_modules/@fortawesome/fontawesome-free/svgs/brands dist/svgs/fab",
    "copy:far5": "cp -rp node_modules/@fortawesome/fontawesome-free/svgs/regular dist/svgs/far",
    "copy:fas5": "cp -rp node_modules/@fortawesome/fontawesome-free/svgs/solid dist/svgs/fas",
    "copy:flags": "cp -rp node_modules/flagpack/flags/1x1 dist/svgs/flags",
    "start": "sirv dist --no-clear",
    "copy:pages": "cp -rp dist public"
  },
  "files": [
    "dist/svgs/*",
    "dist/*.js*"
  ],
  "devDependencies": {
    "@fds-components/fds-codejar": "^1.0.20",
    "@fds-components/fds-demo-element": "^1.0.1",
    "@fds-components/fds-md-documenter": "^1.0.23",
    "@fds-components/fds-helper": "^1.0.9",
    "@rollup/plugin-json": "^4.1.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "bootstrap-icons": "^1.5.0",
    "flagpack": "^1.0.5",
    "rollup": "^2.3.4",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.12",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "@fds-components/fds-helper": "^1.0.1"
  },
  "svelte": "./src/main.js"
}

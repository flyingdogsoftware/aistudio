{
  "name": "@fds-components/fds-treeview",
  "version": "1.0.50",
  "private": false,
  "scripts": {
    "build": "npm run build:prepare && rollup -c",
    "devdoc": "npm run build:prepare && export MAKE_DOC=true; rollup -c -w",
    "dev": "npm run build:prepare && rollup -c -w",
    "build:prepare": "rm -rf dist && mkdir -p dist/node_modules && npm run copy:node_modules && npm run copy:json_doc",
    "copy:json_doc": "find . -maxdepth 1 -name \"*.json\" ! -name 'package*' ! -name 'COMPONENT_API.json' -type f  -exec cp -- {} dist \\;",
    "copy:node_modules": "cp -rp node_modules/@fds-components dist/node_modules",
    "start": "sirv dist --no-clear",
    "copy:pages": "cp -rp dist public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@fds-components/fds-helper": "^1.0.9",
    "@fds-components/fds-icon": "1.1.*",
    "@fds-components/fds-md-documenter": "^1.0.23",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-svelte3": "^3.3.0",
    "rollup": "^2.3.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "7.0.0",
    "sirv-cli": "1.0.*",
    "svelte": "^3.0.0"
  },
  "svelte": "./src/main.js",
  "componentFile": "./src/TreeView.svelte"
}

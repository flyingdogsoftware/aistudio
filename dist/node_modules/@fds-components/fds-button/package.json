{
  "name": "@fds-components/fds-button",
  "version": "1.1.31",
  "private": false,
  "scripts": {
    "build": "npm run build:prepare && rollup -c",
    "devdoc": "npm run build:prepare && export MAKE_DOC=true; rollup -c -w",
    "dev": "npm run build:prepare && rollup -c -w",
    "build:prepare": "rm -rf dist && mkdir -p dist/node_modules && npm run copy:node_modules && npm run copy:json_doc",
    "copy:json_doc": "find . -maxdepth 1 -name \"*.json\" ! -name 'package*' ! -name 'COMPONENT_API.json' -type f  -exec cp -- {} dist \\;",
    "copy:node_modules": "cp -rp node_modules/@fds-components dist/node_modules/@fds-components",
    "start": "sirv dist --no-clear",
    "copy:pages": "cp -rp dist public"
  },
  "files": [
    "dist/*.js*",
    "dist/*.css*",
    "dist/*.json*"
  ],
  "devDependencies": {
    "@fds-components/fds-icon": "1.1.*",
    "@fds-components/fds-demo-element": "^1.0.1",
    "@fds-components/fds-md-documenter": "^1.0.23",
    "@fds-components/fds-helper": "^1.0.9",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-commonjs": "17.1.*",
    "@rollup/plugin-node-resolve": "11.2.*",
    "rollup": "2.*",
    "rollup-plugin-copy": "3.4.*",
    "rollup-plugin-css-only": "3.1.*",
    "rollup-plugin-livereload": "2.0.*",
    "rollup-plugin-svelte": "7.1.*",
    "rollup-plugin-terser": "7.0.*",
    "sirv-cli": "1.0.*",
    "svelte": "3.*"
  },
  "svelte": "./src/main.js"
}

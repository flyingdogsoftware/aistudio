{
  "name": "@fds-components/fds-tippy",
  "version": "1.0.14",
  "devDependencies": {
    "@fds-components/fds-md-documenter": "^1.0.23",
    "@fds-components/fds-helper": "^1.0.9",
    "@rollup/plugin-json": "^4.1.0",
    "@babel/core": "~7.8.7",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/plugin-transform-runtime": "~7.8.3",
    "@babel/preset-env": "~7.8.7",
    "autoprefixer": "~9.7.4",
    "cz-conventional-changelog": "~3.1.0",
    "lint-staged": "~10.0.8",
    "npm-run-all": "~4.1.5",
    "postcss": "~7.0.27",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "semantic-release": "~17.0.4",
    "sirv-cli": "~0.4.5",
    "svelte": "~3.19.2",
    "svelte-preprocess": "~3.4.0"
  },
  "dependencies": {
    "tippy.js": "~6.0.1"
  },
  "scripts": {
    "build": "npm run build:prepare && rollup -c",
    "devdoc": "npm run build:prepare && export MAKE_DOC=true; rollup -c -w",
    "dev": "npm run build:prepare && rollup -c -w",
    "build:prepare": "rm -rf dist && mkdir -p dist/node_modules && npm run copy:node_modules && npm run copy:json_doc",
    "copy:json_doc": "find . -maxdepth 1 -name \"*.json\" ! -name 'package*' ! -name 'COMPONENT_API.json' -type f  -exec cp -- {} dist \\;",
    "copy:node_modules": "cp -rp node_modules/@fds-components dist/node_modules",
    "copy:styles": "cp src/jodit.es2018.css dist",
    "start": "sirv dist --no-clear",
    "copy:pages": "cp -rp dist public"
  },
  "browserslist": [
    "defaults"
  ],
  "keywords": [
    "tooltip",
    "popover",
    "tippy",
    "tippy.js"
  ],
  "files": [
    "src",
    "dist"
  ],
  "svelte": "./src/main.js"
}
